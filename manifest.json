{
    
    "name": "SoundCloud Autoload",
    "author": "HexedHero",
    "version": "1.0.1",
    "description": "Autoload all songs in a playlist.",
    "manifest_version": 3,

    "content_scripts":
    [

        {

            "matches": ["*://*.soundcloud.com/*"],
            "js": ["inject.js"],
            "RunAt": "document_start"

        }

    ],
    "icons":
    { 

        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"

    }
    
}
